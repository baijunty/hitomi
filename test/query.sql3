select * from GalleryFile where gid=2656649 ORDER by name
select * from GalleryFile gf,json_each(gf.tag) ja where  gid=2656649 and json_valid(gf.tag)=1 
select * from UserLog where type= 1 << 17 
SELECT SUM("pgsize")/1024/1024,name FROM "dbstat" group by name 